<?xml version="1.0"  ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/digitalCollege/post">
        <service class="DigitalCollege\Dev\Api\PostManagementInterface" method="getPost" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/custom/products" method="GET">
        <service class="DigitalCollege\Dev\Api\Data\CategoryLinkManagementInterface" method="getAssignedProducts" />
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/custom/products/:id" method="GET">
        <service class="DigitalCollege\Dev\Api\Data\ProductLinkManagementInterface" method="getProductsbyId" />
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
